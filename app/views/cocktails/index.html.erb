<h1>Time to Bartend!</h1>
<p><a href="/cocktails/new">Create and Add a New Cocktail</a></p>
<hr>
<h2>All myCocktails</h2>
<% @cocktails.each do |cocktail| %>
  <div class="row">
    <div class="col s12 m6 l3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="cocktail-name"><%= cocktail.name %></span>
        </div>
        <div class="card-action">
          <a href="/cocktails/<%= cocktail.id %>">View</a>
          <a href="/cocktails/<%= cocktail.id %>/edit">Edit</a>
        </div>
      </div>
    </div>
  
  
<% end %>
</div>